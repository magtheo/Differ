{
  "name": "differ",
  "displayName": "Differ",
  "description": "Apply LLM-generated code changes safely with preview and rollback capabilities",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magtheo/differ.git"
  },
  "bugs": {
    "url": "https://github.com/magtheo/differ/issues"
  },
  "homepage": "https://github.com/magtheo/differ#readme",
  "keywords": [
    "ai",
    "llm",
    "code-generation",
    "diff",
    "preview",
    "chatgpt",
    "claude",
    "automation",
    "refactoring",
    "code-changes"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "differ.openPanel",
        "title": "Open Differ",
        "category": "Differ"
      },
      {
        "command": "differ.applyChanges",
        "title": "Apply Changes",
        "category": "Differ"
      },
      {
        "command": "differ.clearChanges",
        "title": "Clear Changes",
        "category": "Differ"
      },
      {
        "command": "differ.showHistory",
        "title": "Show Change History",
        "category": "Differ"
      },
      {
        "command": "differ.undoLastChanges",
        "title": "Undo Last Changes",
        "category": "Differ"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "differ",
          "title": "Differ",
          "icon": "$(diff)"
        }
      ]
    },
    "views": {
      "differ": [
        {
          "type": "webview",
          "id": "differ-panel",
          "name": "Code Patcher",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "Differ",
      "properties": {
        "differ.logLevel": {
          "type": "string",
          "enum": ["debug", "info", "warn", "error"],
          "default": "info",
          "description": "Set the logging level for the extension"
        },
        "differ.autoBackup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically create backups before applying changes"
        },
        "differ.confirmBeforeApply": {
          "type": "boolean",
          "default": true,
          "description": "Show confirmation dialog before applying changes"
        },
        "differ.maxHistoryEntries": {
          "type": "number",
          "default": 100,
          "description": "Maximum number of history entries to keep"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "compile-webview": "npm run compile && npm run copy-webview",
    "copy-webview": "mkdir -p out/webview && cp -r src/webview/* out/webview/ 2>/dev/null || (mkdir -p out/webview && echo 'No webview files to copy')",
    "clean": "rm -rf out/ node_modules/.cache",
    "rebuild": "npm run clean && npm run compile-webview",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext .ts",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
  },
  "extensionDependencies": [],
  "__metadata": {
    "id": "differ",
    "publisherId": "your-publisher-id",
    "publisherDisplayName": "Your Publisher Name"
  }
}