{
  "name": "differ",
  "displayName": "Differ",
  "description": "Apply LLM-generated code changes safely with preview and rollback capabilities",
  "version": "0.0.1",
  "publisher": "magtheo",
  "author": {
    "name": "Theo",
    "email": "kuletheo@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magtheo/differ.git"
  },
  "bugs": {
    "url": "https://github.com/magtheo/differ/issues"
  },
  "homepage": "https://github.com/magtheo/differ#readme",
  "keywords": [
    "ai",
    "llm",
    "code-generation",
    "diff",
    "preview",
    "chatgpt",
    "claude",
    "automation",
    "refactoring",
    "code-changes"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "files": [
    "out/**/*",
    "grammars/**/*.wasm",
    "images/**/*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "differ.openPanel",
        "title": "Open Differ",
        "category": "Differ"
      },
      {
        "command": "differ.applyChanges",
        "title": "Apply Changes",
        "category": "Differ"
      },
      {
        "command": "differ.clearChanges",
        "title": "Clear Changes",
        "category": "Differ"
      },
      {
        "command": "differ.showHistory",
        "title": "Show Change History",
        "category": "Differ"
      },
      {
        "command": "differ.undoLastChanges",
        "title": "Undo Last Changes",
        "category": "Differ"
      },
      {
        "command": "differ.view.showExample",
        "title": "Show Example Format",
        "category": "Differ"
      },
      {
        "command": "differ.view.showHelp",
        "title": "Show Format Documentation",
        "category": "Differ"
      },
      {
        "command": "differ.view.toggleQuickStart",
        "title": "Toggle Quick Start Guide",
        "category": "Differ"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "differ",
          "title": "Differ",
          "icon": "$(diff)"
        }
      ]
    },
    "views": {
      "differ": [
        {
          "type": "webview",
          "id": "differ-panel",
          "name": "Code Patcher",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "Differ",
      "properties": {
        "differ.logLevel": {
          "type": "string",
          "enum": [
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info",
          "description": "Set the logging level for the extension"
        },
        "differ.autoBackup": {
          "type": "boolean",
          "default": true,
          "description": "Automatically create backups before applying changes"
        },
        "differ.confirmBeforeApply": {
          "type": "boolean",
          "default": true,
          "description": "Show confirmation dialog before applying changes"
        },
        "differ.maxHistoryEntries": {
          "type": "number",
          "default": 100,
          "description": "Maximum number of history entries to keep"
        }
      }
    },
    "submenus": [
      {
        "id": "differViewActionsSubmenu",
        "label": "Differ Options",
        "icon": "$(ellipsis)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "when": "view == differ-panel",
          "group": "navigation",
          "submenu": "differViewActionsSubmenu"
        }
      ],
      "differViewActionsSubmenu": [
        {
          "command": "differ.view.showExample",
          "group": "1_help@1"
        },
        {
          "command": "differ.view.showHelp",
          "group": "1_help@2"
        },
        {
          "command": "differ.view.toggleQuickStart",
          "group": "3_actions@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "compile-webview": "npm run compile && npm run copy-webview && npm run copy-grammars",
    "copy-webview": "mkdir -p out/webview && cp -r src/webview/* out/webview/ 2>/dev/null || (mkdir -p out/webview && echo 'No webview files to copy')",
    "copy-grammars": "mkdir -p out/grammars && cp -r grammars/*.wasm out/grammars/ 2>/dev/null || (mkdir -p out/grammars && echo 'No grammar files to copy')",
    "clean": "rm -rf out/ node_modules/.cache",
    "rebuild": "npm run clean && npm run compile-webview",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext .ts",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "web-tree-sitter": "^0.25.6"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "extensionDependencies": [],
  "__metadata": {
    "id": "magtheo.differ",
    "publisherId": "magtheo",
    "publisherDisplayName": "magtheo"
  }
}